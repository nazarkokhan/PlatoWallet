{
  "Serilog": {
    "MinimumLevel": {
      "Override": {
        "Microsoft.EntityFrameworkCore.Database.Command": "Information",
      }
    },
    "WriteTo": {
      "Main": {
        "Args": {
          "indexFormat": "platipus-test",
        }
      },
      "Local": {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:HH:mm:ss.fffffff} {Level:u3}] <source:{SourceContext}, properties:{Properties}>{NewLine}{Message:lj}{NewLine}{Exception}{NewLine}"
        }
      }
    },
  },
  "ConnectionStrings": {
    //    "WalletDbContext": "Server=localhost;Port=5432;Database=platipus_wallet_db;Username=postgres;Password=1;Include Error Detail=true",
    "RedisCache": "localhost"
  }
}
