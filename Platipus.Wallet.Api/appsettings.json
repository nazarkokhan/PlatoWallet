{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "Microsoft.AspNetCore.Hosting.Diagnostics": "Warning",
        "Microsoft.AspNetCore.Routing.EndpointMiddleware": "Warning",
        "Microsoft.AspNetCore.Mvc.Formatters.XmlSerializerOutputFormatter": "Fatal"
      }
    },
    "SelfLogFilePath": "./logs/self.txt",
    "WriteTo": {
      "Main": {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://elastic.aws.intra:9200;",
          "indexFormat": "platipus-wallet-api",
          "connectionGlobalHeaders": "Authorization=Basic cGxhdGlwdXNfZWxhc3RpYzpUaGFpcmFoUGgydXNob28=",
          "customFormatter": {
            "$type": "Platipus.Serilog.TargetedElasticsearchJsonFormatter, Platipus.Serilog"
          },
          "emitEventFailure": "WriteToSelfLog, WriteToFailureSink",
          "failureSink": {
            "$type": "Serilog.Sinks.File.FileSink, Serilog.Sinks.File",
            "path": "./logs/serilog-failed.txt",
            "textFormatter": "Platipus.Serilog.TargetedElasticsearchJsonFormatter, Platipus.Serilog",
            "fileSizeLimitBytes": null
          },
          "autoRegisterTemplateVersion": "ESv7",
          "batchAction": "Create",
          "typeName": null
        }
      }
    },
    "Enrich": [
